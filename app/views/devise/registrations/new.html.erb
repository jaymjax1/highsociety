<div class="container col-xs-6 col-xs-offset-3">
  <div class="panel panel-default">
    <div class="panel-body">
      <h2>Sign up</h2>

      <%= bootstrap_form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
      <%= devise_error_messages! %>
      <div class="container col-xs-8 col-xs-offset-2">
        <div class="row">

          <div class ="col-xs-6">
            <%= f.text_field :first_name, autofocus: true %>
          </div>
          <div class="col-xs-6">
            <%= f.text_field :last_name, autofocus: true %>  
          </div>
        </div>

        <div class="row">

          <div class ="col-xs-6">
            <%= f.email_field :email, autofocus: true %>
          </div>
          <div class="col-xs-6">
            <%= f.text_field :username, autofocus: true %>
          </div>
        </div>

        <div class="row">

          <div class ="col-xs-6">
            <%= f.password_field :password,autocomplete: "off" %>
          </div>
          <div class="col-xs-6">
            <%= f.password_field :password_confirmation, autocomplete: "off" %> 
          </div>
        </div>

        <div class="row">

          <div class ="col-xs-6">
            <%= f.text_field :street, autofocus: true %>
          </div>
          <div class="col-xs-6">
            <%= f.text_field :city,autofocus: true %>  
          </div>
        </div>

        <div class="row">

          <div class ="col-xs-6">
            <%= f.text_field :state,autofocus: true %>
          </div>
          <div class="col-xs-6">
            <%= f.text_field :zip_code, autofocus: true %> 
          </div>
        </div>

        <div class="row">
          <div class ="col-xs-12">
            <%= f.text_area :about_me, :cols =>47, :rows =>8, autofocus: true %><br />
          </div>
        </div>


        <div class="actions">
          <%= f.submit "Sign up" %>
        </div>
        <% end %>

      </div>
    </div>
  </div>
</div>

